ed by {singleFinishedProject.orderFrom?.name}</ProjectDescription>\n                        <ProjectDescription>Order Date {singleFinishedProject.createdDate}</ProjectDescription>\n                        <ProjectDescription>Review {singleFinishedProject.review}</ProjectDescription>\n                        <ProjectDescription>Rating {singleFinishedProject.rating}</ProjectDescription>\n                        <ProjectDescription>\n                            Project Status: <Tag color={'green'}> Finished</Tag>\n                        </ProjectDescription>\n                    </ProjectContainer>\n                );\n            })}\n        </div>\n    );\n};\n\nconst ReviewList = ({ reviews }) => {\n    return (\n        <div>\n            {reviews.map((singleReview) => {\n                return (\n                    <ProjectContainer>\n                        <ProjectTitle>{singleReview.review}</ProjectTitle>\n                        <ProjectDescription> Reviewed by: {singleReview.reviewBy?.name}</ProjectDescription>\n                        <Rate allowHalf defaultValue={singleReview.rating} value={singleReview.rating} />\n                    </ProjectContainer>\n                );\n            })}\n        </div>\n    );\n};\n\nconst ServicesOfferedList = ({ offeredServices }) => {\n    return (\n        <div>\n            {offeredServices.map((singleService) => {\n                return (\n                    <ProjectContainer>\n                        <ProjectTitle>{singleService.name}</ProjectTitle>\n                        <Tag color={'blue'}>{singleService.type}</Tag>\n                        <ProjectDescription>{singleService.description}</ProjectDescription>\n                        <ProjectDescription>Rate: {singleService.hourlyRate}$</ProjectDescription>\n                        {/*<ProjectDescription>Service type: {singleService.type}</ProjectDescription>*/}\n                    </ProjectContainer>\n                );\n            })}\n        </div>\n    );\n};\n\nconst ProjectContainer = styled.div`\n    border: 1px solid #e6e8e7;\n    padding: 10px;\n    margin: 10px 0px;\n`;\n\nconst ProjectTitle = styled.div`\n    font-size: 16px;\n    font-weight: bold;\n`;\nconst ProjectDescription = styled.div`\n    font-size: 14px;\n    color: grey;\n`;\n\nconst ServiceProviderName = styled.div`\n    font-size: 20px;\n    font-weight: bold;\n`;\n\nconst ServiceProviderRating = styled.div`\n    font-size: 16px;\n    color: grey;\n`;\n","/Users/faisal/Documents/Development/toptal-assignment/toptal-assignment-frontend/src/store/service/requests/CreateServiceRequest.ts",[],"/Users/faisal/Documents/Development/toptal-assignment/toptal-assignment-frontend/src/store/user/UserAction.ts",[],"/Users/faisal/Documents/Development/toptal-assignment/toptal-assignment-frontend/src/store/user/UserEffect.ts",[],"/Users/faisal/Documents/Development/toptal-assignment/toptal-assignment-frontend/src/views/forms/useFormInputValidation.ts",["359","360"],"import { useEffect, useState } from 'react'\nimport { message } from 'antd'\nimport * as Yup from 'yup'\nimport {AnyObjectSchema, ValidationError} from \"yup\";\n\n\ninterface IErrorObject {\n    [key: string]: string;\n}\n\ninterface ITouchedObject {\n    [key: string]: boolean;\n}\n\nexport interface IControl{\n    values:any,\n    touched: ITouchedObject,\n    errors:IErrorObject,\n    handleSubmit : (onSubmit: () =>void) => void,\n    handleInputChange: (a:string , b:any) => void,\n    setSingleValue :(name:string, value:string|number|boolean) =>  void,\n    setAllValues :(newValues:any) =>  void,\n    resetData:() =>  void,\n}\n\n\nexport default function useFormInputValidation<T>(\n    INITIAL_STATE:T,\n    VALIDATION_SCHEMA: AnyObjectSchema= Yup.object({})\n) {\n    const [values, setValues] = useState(INITIAL_STATE)\n    const [touched, setTouched] = useState<ITouchedObject>({})\n    const [hasError, setHasError] = useState<boolean>(false)\n    const [errors, setErrors] = useState<IErrorObject>({})\n\n    const handleInputChange = (name:string, value:string|number|boolean) => {\n        const newValues = {...values, [name]: value}\n        const newTouched= {...touched, [name]: true}\n\n        setValues(newValues)\n        setTouched(newTouched)\n    }\n\n    const setAllValues = (newValues:T) => {\n        setValues(newValues)\n    }\n\n    const setSingleValue = (name:string, value:string|number|boolean) => {\n        setValues({\n            ...values,\n            [name]: value\n        })\n    }\n\n    const handleSubmit = (onSubmit: () =>void) => {\n        if (!hasError) {\n            onSubmit()\n        } else {\n            const touched:ITouchedObject = {}\n            Object.keys(INITIAL_STATE).map((key) => {\n                touched[key] = true\n            })\n            setTouched(touched)\n            message.error('Please fill up all the required fields')\n        }\n    }\n\n    useEffect(() => {\n        validateData()\n    }, [values])\n\n    const validateData = () => {\n        if (VALIDATION_SCHEMA.validate) {\n            VALIDATION_SCHEMA\n                .validate(values, { abortEarly: false })\n                .then(() => {\n                    setErrors({})\n                    setHasError(false)\n                })\n                .catch((err) => {\n                    const errorObject: IErrorObject= {}\n                    err.inner.forEach((item:ValidationError) => (item.path? errorObject[item.path] = item.message: console.log('path not found')))\n                    setErrors(errorObject)\n                    setHasError(true)\n                })\n        }\n    }\n\n    const resetData = () => {\n        setValues(INITIAL_STATE)\n        setTouched({})\n    }\n\n    const control:IControl ={\n        values,\n        touched,\n        errors,\n        handleSubmit,\n        handleInputChange,\n        setSingleValue,\n        setAllValues,\n        resetData\n    }\n\n    return {\n        values,\n        hasError,\n        errors,\n        handleSubmit,\n        handleInputChange,\n        resetData,\n        setValues,\n        control\n    }\n}\n","/Users/faisal/Documents/Development/toptal-assignment/toptal-assignment-frontend/src/views/forms/FormInputText.tsx",[],"/Users/faisal/Documents/Development/toptal-assignment/toptal-assignment-frontend/src/views/pages/user/ModalAuthentication.tsx",["361"],"/Users/faisal/Documents/Development/toptal-assignment/toptal-assignment-frontend/src/views/forms/SelectionItemModel.js",[],"/Users/faisal/Documents/Development/toptal-assignment/toptal-assignment-frontend/src/views/common-components/modals/ModalDeleteConfirmation.js",["362"],"/Users/faisal/Documents/Development/toptal-assignment/toptal-assignment-frontend/src/views/pages/user/TableUsers.tsx",["363"],"/Users/faisal/Documents/Development/toptal-assignment/toptal-assignment-frontend/src/views/pages/service/TableServices.tsx",["364","365"],"/Users/faisal/Documents/Development/toptal-assignment/toptal-assignment-frontend/src/views/pages/PageSuperAdmin.tsx",[],"/Users/faisal/Documents/Development/toptal-assignment/toptal-assignment-frontend/src/store/order/OrderAction.ts",[],"/Users/faisal/Documents/Development/toptal-assignment/toptal-assignment-frontend/src/store/order/OrderEffect.ts",[],"/Users/faisal/Documents/Development/toptal-assignment/toptal-assignment-frontend/src/store/order/OrderReducer.ts",[],"/Users/faisal/Documents/Development/toptal-assignment/toptal-assignment-frontend/src/views/pages/order/TableOrders.tsx",["366"],"/Users/faisal/Documents/Development/toptal-assignment/toptal-assignment-frontend/src/views/pages/order/TableReviews.tsx",["367","368","369","370"],"import React, { FC, useEffect } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { Table, Tag } from 'antd';\nimport { OrderAction } from '../../../store/order/OrderAction';\nimport { selectReviews } from '../../../store/order/OrderSelector';\nimport { OrderStatus } from '.